{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "google/recaptcha": "~1.1",
        "guzzlehttp/guzzle": "5.3.*|6.0.*",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6",
        "nesbot/carbon": "^2",
        "pusher/pusher-php-server": "~3.0",
        "webpatser/laravel-uuid": "2.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "t": [
            "[ -e vendor/bin/phpunit ] || ( echo 'PHPUnit is not installed' && exit 1 )",
            "touch storage/testing.sqlite || ( echo 'Unable to create storage/testing.sqlite' && exit 2 )",
            "php artisan migrate --env=testing --database=sqlite_testing --force",
            "php artisan config:clear || ( echo 'Error removing cache' && exit 3 )",
            "[ -e .env ] && mv .env .env.active && cp .env.testing .env",
            "vendor/bin/phpunit; echo $? > /tmp/phpunit-testing-error-code",
            "[ -e .env.active ] && mv .env.active .env",
            "php artisan config:cache || ( echo 'Error creating cache' && exit 4 )",
            "rm storage/testing.sqlite || ( echo 'Unable to remove storage/testing.sqlite' && exit 5 )",
            "e=$(cat /tmp/phpunit-testing-error-code); exit $e"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
